# PostgreSQL
DATABASE_URL=postgresql+asyncpg://dbuser:dbpassword@localhost:5432/mockbank

# Redis for rate-limiting
REDIS_URL=redis://localhost:6379/0

# Master API key used to create new API keys via admin process
MASTER_API_KEY=changeme_master_key

# Flask/Secret analogue for signing tokens if needed (not required for API key auth)
SECRET_KEY=changeme_secret_key

# Rate limiting: requests per minute per api key
RATE_LIMIT="60" 

# Gunicorn workers (for start.sh)
GUNICORN_WORKERS=4
